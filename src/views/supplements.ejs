<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
    integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  <title>Document</title>
</head>

<body>
  <div class="container-fluid">
    <form method="post" action="/app">
      <div class="row mt-4">
        <div class="col-md-4">
          <div class="form-group ">
            <select class="form-control" name="type" id="select-field">
              <option>Supplement</option>
              <option>Drug</option>
            </select>
          </div>
        </div>
        <div class="col-md-4">
          <div class="form-group">
            <input class="form-control" list="supplements" name="name" id="name-field">
            <datalist id="supplements">

            </datalist>
          </div>
        </div>
        <div class="col-md-2">
          <button type="submit" class="btn btn-primary">Add</button>
        </div>
      </div>
    </form>
    <div class="row">
      <div class="col-md-2">
        <h4>Supplement</h4>
        <% supp.forEach(function(item){ %>

        <%= item.name %>
        <% }) %>
      </div>
      <div class="col-md-8 text-center">
        <h4>Visualizations over here</h4>
      </div>
      <div class="col-md-2">
        <h4>Drug</h4>
        <% drug.forEach(function(item){ %>
        <%= item.name %>
        <% }) %>
      </div>
    </div>
  </div>
</body>
<script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
<script>
  $(document).ready(function () {
    $("#name-field").on("change keydown paste input", function () {
      var type = $("#select-field").val();
      var type_val;
      if (type == "Supplement") {
        type_val = 1;
      } else {
        type_val = 2
      }
      console.log(type_val);

      $.ajax({
        type: 'GET',
        crossDomain: true,
        // dataType: 'jsonp',
        url: 'http://localhost:8080/KeyWords?type=' + type_val + '&name=ab',
        success: function (jsondata) {

          $("#supplements").empty();

          $.each(jsondata, function (key, value) {
            console.log(value);
            $("#supplements").append("<option value=\"" + value + "\">")
          })
        }
      })
    });
  })
  // function getKeyWords() {

  // }
</script>

</html>